{
    "name": "made-foryou/routes",
    "description": "A packages which can provide routing generated from the database.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MadeForYou\\Routes\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Menno Tempelaar",
            "email": "menno@made-foryou.nl",
            "role": "developer"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "./../filament-categories-package"
        },
        {
            "type": "path",
            "url": "./../filament-helpers"
        },
        {
            "type": "path",
            "url": "./../filament-news-package"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~8.3",
        "spatie/laravel-package-tools": "^1.16",
        "filament/filament": "^3.2"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^8.21",
        "made-foryou/filament-categories": "dev-main",
        "made-foryou/filament-news": "dev-main"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadeForYou\\Routes\\RoutesServiceProvider"
            ],
            "aliases": {
                "Routing": "MadeForYou\\Routes\\Facades\\Routing"
            }
        }
    }
}
